<h1>Nuevo articulo</h1>
<div class="container">
  <div class="">
    <%= form_with(model: @article) do |form| %>
        <%= form.label "Titulo" %>
        <%= form.text_field :title %>
        <%= form.label "Cuerpo" %>
        <%= form.text_area :body %>
        <%= form.submit "Enviar" %>
    <%end%>
  </div>
</div>
